class emp
{
	int eno=101;
	void display()
	{
		eno=300;
		System.out.println(eno);
	}
}
class manager extends emp
{
	void display()
	{
		System.out.println("Manager class");
	}
}
class Tes1 {

	public static void main(String[] args) 
	{
		// TODO Auto-generated method stub
		emp e=new emp();
        e.display();
        emp d=new manager();
        d.display();

	}

}
