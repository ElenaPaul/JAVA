class Student
{
	String name;
	Student(String s){}
	void setName(String na)
	{
		name=na;
	}
	String getName()
	{
		return name;
	}
}

public class Test {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Student s1=new Student("John");
		change(s1);
		System.out.println("s1="+s1.getName());
	}

	public static void change(Student p)
	{
		p.setName("Mary");
	}

}
