public class Name {

	public static void main(String[] s) 
	{
		// TODO Auto-generated method stub
		int length=0;
		try
		{
			length=s[0].length()+s[1].length();
			if(length<10)
				return;

			System.out.println("Name length should be less than 10");

		}
		catch(ArrayIndexOutOfBoundsException e)
		{
			System.out.println("2 command line arguments required");
		}
		finally
		{
			System.out.println("Length of " +s[0] + s[1]+ " is " +length);
			System.out.println("Thank you!");
		}
	}
}
